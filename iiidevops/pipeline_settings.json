{
  "name": "Spring Boot with PostgreSQL - simple api",
  "description": "SpringBoot 2.2.1.RELEASE / Gradle jdk8 / PostgreSQL 10.2",
  "arguments": [
    {
      "key": "db.username",
      "display": "DB Username",
      "default_value": "iiidevops",
      "input_type": "text"
    },
    {
      "key": "db.password",
      "display": "DB Password",
      "default_value": "templatePassword",
      "input_type": "password"
    },
    {
      "key": "db.name",
      "display": "DB Name",
      "default_value": "todo_db",
      "input_type": "text"
    }
  ]
}
