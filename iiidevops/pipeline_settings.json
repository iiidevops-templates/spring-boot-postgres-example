    },
    {
      "key": "db.password",
      "display": "DB Password",
      "default_value": "templatePassword",
      "input_type": "password"
    },
    {
      "key": "db.name",
      "display": "DB Name",
      "default_value": "todo_db",
      "input_type": "text"
    }
